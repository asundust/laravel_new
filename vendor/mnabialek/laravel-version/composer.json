{
    "name": "mnabialek/laravel-version",
    "description": "Get Laravel/Lumen version",
    "keywords": [
        "laravel",
        "lumen",
        "laravel version",
        "lumen version"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Nabia≈Çek",
            "email": "dev@nabialek.org"
        }
    ],
    "require": {
        "illuminate/container": "5.*|6.*|7.*|8.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9",
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mnabialek\\LaravelVersion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mnabialek\\LaravelVersion\\Tests\\": "tests"
        }
    }
}
